<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data>
        <record id="as_sales_order_seq" model="ir.sequence">
            <field name="name">Order Number Sequence</field>
            <field name="code">as.sale.order</field>
            <field name="prefix">SO/%(day)s/%(month)s/%(year)s/</field>
            <field name="padding">3</field>
            <field eval="True" name="use_date_range"/>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>